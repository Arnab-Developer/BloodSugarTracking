@using BloodSugarTracking.Models
@model BloodSugarTestResult
@{
    ViewData["Title"] = "Edit blood sugar result";
}
<h1>Blood sugar results</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-controller="BloodSugar" asp-action="Edit" method="post">
            <input type="hidden" asp-for="Id" />
            <div class="form-group">
                <label asp-for="MealTime" class="control-label"></label>
                <input asp-for="MealTime" class="form-control" />
                <span asp-validation-for="MealTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="TestTime" class="control-label"></label>
                <input asp-for="TestTime" class="form-control" />
                <span asp-validation-for="TestTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Result" class="control-label"></label>
                <input asp-for="Result" class="form-control" />
                <span asp-validation-for="Result" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Update" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
<div>
    <a asp-controller="BloodSugar" asp-action="Index">Back to List</a>
</div>

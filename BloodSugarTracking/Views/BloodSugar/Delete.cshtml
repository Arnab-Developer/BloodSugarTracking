@model BloodSugarTestResult
@{
    ViewData["Title"] = "Delete blood sugar result";
}
<h1>Blood sugar results</h1>
<hr />
<div class="row">
    <div class="col-md-4">
        <div class="form-group">
            @Html.DisplayNameFor(model => model.MealTime)
            @Html.DisplayFor(model => model.MealTime)
        </div>
        <div class="form-group">
            @Html.DisplayNameFor(model => model.TestTime)
            @Html.DisplayFor(model => model.TestTime)
        </div>
        <div class="form-group">
            @Html.DisplayNameFor(model => model.Result)
            @Html.DisplayFor(model => model.Result)
        </div>

        <form asp-controller="BloodSugar" asp-action="Delete" method="post">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Delete" class="btn btn-danger" />
        </form>
    </div>
</div>
<div>
    <a asp-controller="BloodSugar" asp-action="Index">Back to List</a>
</div>
